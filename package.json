{
    "name": "multi-input-component",
    "version": "0.1.0",
    "private": true,
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Firefox versions",
        "last 2 Edge versions",
        "last 2 Safari versions",
        "last 2 ios_saf versions",
        "last 2 ChromeAndroid versions"
    ],
    "prettier": {
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all"
    },
    "lint-staged": {
        "*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged",
        "*.{css,scss}": "stylelint",
        "*.{js,jsx}": [
            "eslint --fix",
            "stylelint"
        ]
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "deploy": "npm run build && firebase deploy",
        "lint:js": "eslint --fix --ext .jsx,.js ./**/",
        "lint:css": "stylelint './**/*.jsx'",
        "cleanup": "rm yarn.lock; rm package-lock.json; rm -rf node_modules",
        "reboot": "npm run cleanup; npm install"
    },
    "dependencies": {
        "prop-types": "^15.6.2",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-scripts": "1.1.4",
        "styled-components": "^3.3.3",
        "styled-normalize": "^8.0.0",
        "styled-system": "^2.3.3"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.5",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-plugin-compat": "^2.4.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.1.0",
        "eslint-plugin-react": "^7.10.0",
        "eslint-plugin-unicorn": "^4.0.3",
        "eslint": "^5.0.1",
        "husky": "^0.14.3",
        "imagemin-lint-staged": "^0.3.0",
        "lint-staged": "^7.2.0",
        "prettier": "^1.13.7",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-no-unsupported-browser-features": "^3.0.0",
        "stylelint-order": "^0.8.1",
        "stylelint-processor-styled-components": "^1.3.1",
        "stylelint": "^9.3.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nygdjs/multi-input"
    }
}
